<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="924.0"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="automatization.controllers.ClientBaseController">
    <children>
        <!-- Таблица в которую мы будем выводит наш pojo -->
        <TableView fx:id="tableClientBaseVariable" layoutX="12.0" layoutY="62.0" prefHeight="324.0" prefWidth="850.0">
            <columns>
                <!-- Колонки таблицы -->
                <TableColumn prefWidth="80.0" text="№" fx:id="idColumn" />
                <TableColumn prefWidth="80.0" text="Name" fx:id="loginColumn" />
                <TableColumn prefWidth="80.0" text="Surname" fx:id="surnameColumn" />
                <TableColumn prefWidth="100.0" text="Birthdate" fx:id="birthdateColumn" />
                <TableColumn prefWidth="100.0" text="Passport" fx:id="passportColumn" />
                <TableColumn prefWidth="150.0" text="Pickup point " fx:id="passVidachaColumn" />
                <TableColumn prefWidth="100.0" text="Email:" fx:id="emailColumn" />


                <TableColumn prefWidth="80.0" text="City" fx:id="cityColumn" />
                <TableColumn prefWidth="100.0" text="Address" fx:id="addressColumn" />
                <TableColumn prefWidth="100.0" text="Phone" fx:id="phoneColumn" />
            </columns>
        </TableView>
        <Label layoutX="14.0" layoutY="18.0" text="Клиентская база предприятия">
            <font>
                <Font name="Verdana" size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>